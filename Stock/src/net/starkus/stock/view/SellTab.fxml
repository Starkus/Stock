<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<?import net.starkus.stock.control.AutoCompleteTextField?>

<AnchorPane prefHeight="638.0" prefWidth="1054.0" styleClass="background"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="net.starkus.stock.view.SellTabController">
	<children>
		<VBox prefHeight="200.0" prefWidth="100.0" spacing="16.0"
			AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<children>
				<HBox prefHeight="488.0" prefWidth="762.0" spacing="10.0"
					VBox.vgrow="ALWAYS">
					<children>
						<VBox prefHeight="484.0" prefWidth="287.0" styleClass="panel">
							<children>
								<Label alignment="TOP_CENTER" prefWidth="2000.0"
									styleClass="label-title" text="%label.lowstock">
									<padding>
										<Insets bottom="20.0" />
									</padding>
								</Label>
								<TableView fx:id="lowStockTable" prefHeight="200.0"
									prefWidth="200.0" VBox.vgrow="ALWAYS">
									<columns>
										<TableColumn fx:id="lowStockNameColumn" prefWidth="75.0"
											text="%product" />
										<TableColumn fx:id="lowStockAvailableColumn"
											maxWidth="3000.0" prefWidth="90.0" text="%available" />
									</columns>
									<columnResizePolicy>
										<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
									</columnResizePolicy>
								</TableView>
							</children>
							<padding>
								<Insets bottom="10.0" left="10.0" right="10.0" top="15.0" />
							</padding>
						</VBox>
						<VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="SOMETIMES">
							<children>
								<Label alignment="TOP_CENTER" prefWidth="2000.0"
									style="-fx-background-color: #16a085; -fx-background-radius: 3px 3px 0 0;"
									styleClass="label-title" text="%label.shoppingcart">
									<padding>
										<Insets bottom="15.0" top="15.0" />
									</padding>
									<VBox.margin>
										<Insets />
									</VBox.margin>
								</Label>
								<TableView fx:id="productTable" prefHeight="200.0"
									prefWidth="200.0" style="-fx-background-color: #1abc9c;"
									VBox.vgrow="SOMETIMES">
									<columns>
										<TableColumn fx:id="productNameColumn" prefWidth="-1.0"
											text="%product" />
										<TableColumn fx:id="ammountColumn" maxWidth="2000.0"
											prefWidth="-1.0" text="%ammount" />
										<TableColumn fx:id="priceColumn" maxWidth="2000.0"
											prefWidth="75.0" text="%price" />
										<TableColumn fx:id="subtotalColumn" maxWidth="2000.0"
											prefWidth="75.0" text="%subtotal" />
										<TableColumn fx:id="actionColumn" maxWidth="60.0" minWidth="60.0" 
											prefWidth="75.0" />
									</columns>
									<columnResizePolicy>
										<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
									</columnResizePolicy>
								</TableView>
								<HBox prefWidth="200.0">
									<children>
										<AutoCompleteTextField fx:id="addProductField"
											maxHeight="-Infinity" prefHeight="36.0" promptText="%prompt.addproduct"
											style="-fx-background-radius: 0 0 0 3; -fx-background-image: url(/net/starkus/stock/media/product_field.png); -fx-font-family: Nato; -fx-font-size: 14pt; -fx-padding: 0 0 0 40px;"
											styleClass="text-field-withicon" HBox.hgrow="ALWAYS" />
										<Button fx:id="addProductButton" maxHeight="-Infinity" maxWidth="-Infinity"
											mnemonicParsing="false" prefHeight="36.0" prefWidth="47.0"
											style="-fx-background-radius: 0 0 3 0; -fx-font-size: 24; -fx-padding: 0;"
											text="+" textFill="WHITE" />
									</children>
								</HBox>
							</children>
						</VBox>
						<VBox prefHeight="638.0" prefWidth="261.0" styleClass="panel">
							<children>
								<Label alignment="TOP_CENTER" prefWidth="2000.0"
									styleClass="label-title" text="%label.account">
									<VBox.margin>
										<Insets bottom="20.0" />
									</VBox.margin>
								</Label>
								<AutoCompleteTextField fx:id="accountField"
									promptText="%prompt.client"
									style="-fx-background-image: url(/net/starkus/stock/media/account_field.png);">
									<styleClass>
										<String fx:value="text-field-withicon" />
										<String fx:value="text-field-dark" />
									</styleClass>
								</AutoCompleteTextField>
								<VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0"
									VBox.vgrow="SOMETIMES">
									<children>
										<Label fx:id="clientNameLabel" styleClass="label-header" text="?" />
										<HBox alignment="CENTER">
											<children>
												<Label text="%label.currentdebt" />
												<Label fx:id="currentDebtLabel" text="?" />
											</children>
											<padding>
												<Insets top="4.0" />
											</padding>
										</HBox>
									</children>
								</VBox>
								<Label alignment="CENTER" prefWidth="2000.0" styleClass="label-title"
									text="%label.paid">
									<padding>
										<Insets bottom="12.0" />
									</padding>
								</Label>
								<TextField fx:id="paidField"
									style="-fx-background-image: url(/net/starkus/stock/media/paid_field.png);">
									<styleClass>
										<String fx:value="text-field-dark" />
										<String fx:value="text-field-withicon" />
									</styleClass>
								</TextField>
								<HBox alignment="CENTER">
									<children>
										<Label text="%label.newdebt" />
										<Label fx:id="newDebtLabel" text="?" />
									</children>
									<padding>
										<Insets top="4.0" />
									</padding>
								</HBox>
							</children>
							<HBox.margin>
								<Insets />
							</HBox.margin>
							<padding>
								<Insets bottom="10.0" left="10.0" right="10.0" top="15.0" />
							</padding>
						</VBox>
					</children>
				</HBox>
				<HBox alignment="CENTER">
					<children>
						<Label styleClass="label-header" text="%label.total">
							<font>
								<Font size="34.0" />
							</font>
						</Label>
						<Label fx:id="totalLabel" styleClass="label-huge" text="\$0.00" />
					</children>
				</HBox>
				<HBox alignment="BOTTOM_RIGHT" spacing="8.0" VBox.vgrow="NEVER">
					<children>
						<Button fx:id="finishButton"
							mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT"
							prefHeight="40.0" prefWidth="120.0" style="-fx-font-size: 20;"
							text="%button.finish" />
						<Button fx:id="clearButton" cancelButton="true" minWidth="90.0"
							mnemonicParsing="false" text="%button.clear" />
					</children>
				</HBox>
			</children>
			<padding>
				<Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
			</padding>
		</VBox>
	</children>
</AnchorPane>
